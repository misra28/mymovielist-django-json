{
    "builds": [{
        "src": "mymovielist/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "mymovielist/wsgi.py",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}